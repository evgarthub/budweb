{
  "connection": "default",
  "collectionName": "requests",
  "info": {
    "name": "request",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "description": {
      "type": "text"
    },
    "status": {
      "enum": [
        "open",
        "review",
        "progress",
        "declined",
        "hold",
        "done"
      ],
      "type": "enumeration",
      "default": "open"
    },
    "user": {
      "model": "user",
      "via": "requests",
      "plugin": "users-permissions"
    }
  }
}